{
  "include": [
    "../lib/*.ts"
  ],
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "lib": [
      "es2015"
    ],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "outDir": ".",
    "skipLibCheck": true,
    "strictNullChecks": true
  }
}
